.ui-widget-content .ui-state-default,
.ui-widget-header .ui-state-default {
    text-shadow: 0px 0px rgb(255, 255, 255);
}

#rightsidebar {
    width: 329px !important;
}



.ui-widget-content:not(#textchat) {
    border-radius: 0px;
    border-color: var(--gnome-tabbar-tab-active-background-contrast);
}

#rightsidebar ul.tabmenu li a {
    color: transparent;
}

.tabmenu {
    margin-top: 5px !important;
}

#textchat,
#imagedialog,
#journal,
#compendium,
#jukebox,
#deckstables,
#vm_settings_categories {
