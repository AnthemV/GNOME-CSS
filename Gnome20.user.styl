    border-top-width: 1px;
    border-color: var(--gnome-tabbar-tab-active-background-contrast) !important;
}


#ui-id-1,
#ui-id-2,
#ui-id-3,
#ui-id-4,
#ui-id-5,
#ui-id-6,
#ui-id-7 {
